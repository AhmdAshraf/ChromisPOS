		<![CDATA[SELECT 
RECEIPTS.DATENEW,
TICKETS.TICKETID,
PAYMENTS.PAYMENT,
PAYMENTS.DEBTDUE,
PAYMENTS.TOTAL,
TICKETLINES.UNITS,
PRODUCTS.NAME,
CUSTOMERS.NAME AS CUSTOMERNAME
FROM RECEIPTS, TICKETS, CUSTOMERS, PAYMENTS, TICKETLINES, PRODUCTS
WHERE RECEIPTS.ID = TICKETS.ID AND RECEIPTS.ID = PAYMENTS.RECEIPT
AND TICKETS.CUSTOMER = CUSTOMERS.ID 
AND TICKETLINES.TICKET = TICKETS.ID
AND PRODUCTS.ID = TICKETLINES.PRODUCT
AND (PAYMENTS.PAYMENT = 'debt' OR PAYMENTS.PAYMENT = 'debtpaid')
AND ?(QBF_FILTER)
ORDER BY CUSTOMERS.NAME, RECEIPTS.DATENEW DESC]]>

